{
	"parser": "babel-eslint",
	"extends": [
		"prettier",
		"prettier/react"
	],
	"env": {
		"browser": true,
		"node": true,
		"mocha": true,
		"es6": true,
		"jest": true
	},
	"globals": {
		"VERSION": true
	},
	"parserOptions": {
		"arrowFunctions": true,
		"binaryLiterals": true,
		"blockBindings": true,
		"defaultParams": true,
		"forOf": true,
		"generators": true,
		"objectLiteralComputedProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"regexUFlag": true,
		"superInFunctions": true,
		"templateStrings": true,
		"unicodeCodePointEscapes": true,
		"modules": true,
		"jsx": true
	},
	"rules": {
		"prettier/prettier": "error",
		"no-var": 2,
		"no-console": 1,
		"consistent-return": 0,
		"guard-for-in": 2,
		"no-eq-null": 2,
		"no-native-reassign": [
			2,
			{
				"exceptions": [
					"Promise"
				]
			}
		],
		"no-self-compare": 2,
		"no-throw-literal": 2,
		"no-unused-expressions": 0,
		"wrap-iife": [
			2,
			"outside"
		],
		"strict": 0,
		"no-shadow": 0,
		"no-unused-vars": [
			2,
			{
				"args": "none"
			}
		],
		"no-use-before-define": [
			2,
			"nofunc"
		],
		"no-mixed-requires": 2,
		"comma-style": [
			2,
			"last"
		],
		"key-spacing": 2,
		"no-underscore-dangle": 0,
		"keyword-spacing": [
			"error",
			{
				"before": true,
				"after": true
			}
		],
		"space-before-blocks": [
			2,
			"always"
		],
		"array-bracket-spacing": [
			2,
			"never"
		],
		"computed-property-spacing": [
			2,
			"never"
		],
		"space-in-parens": [
			2,
			"never"
		],
		"space-unary-ops": [
			2,
			{
				"words": true,
				"nonwords": false
			}
		],
		"spaced-comment": [
			2,
			"always"
		],
		"new-cap": 0,
		"eol-last": 0,
		"func-names": 0,
		"space-before-function-parenthesis": 0,
		"camelcase": 0,
		"brace-style": 0,
		"no-extend-native": 2,
		"linebreak-style": 2,
		"constructor-super": 2,
		"object-shorthand": 0,
		"dot-location": [
			2,
			"property"
		],
		"no-unneeded-ternary": 2,
		"quote-props": [
			2,
			"as-needed"
		],
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2
	},
	"plugins": [
		"react",
		"import",
		"jsx-a11y",
		"prettier",
		"react-hooks"
	]
}